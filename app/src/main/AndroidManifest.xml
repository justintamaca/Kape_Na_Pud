<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.logsignsql">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="31">
        <activity
            android:name=".MilkshakeActivity"
            android:exported="false" />
        <activity
            android:name=".NonicedCoffeeActivity"
            android:exported="false" />
        <activity
            android:name=".HotCoffeeActivity"
            android:exported="false" />
        <activity
            android:name=".IcedCoffeeActivity"
            android:exported="false"/>
        <activity
            android:name=".SandwichActivity"
            android:exported="false" />
        <activity
            android:name=".RiceMealActivity"
            android:exported="false" />
        <activity
            android:name=".AppetizerActivity"
            android:exported="false" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".CartActivity"
            android:exported="false" />
        <activity
            android:name=".MenuActivity"
            android:exported="false" />
        <activity
            android:name=".DashboardActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- SignupActivity -->
        <activity
            android:name=".SignupActivity"
            android:exported="false" /> <!-- MainActivity -->
        <activity
            android:name=".ui.theme.MainActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
    </application>

</manifest>